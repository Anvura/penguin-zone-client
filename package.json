{
   "name": "snowyfieldsclient",
   "version": "0.7.0",
   "description": "Desktop client for snowyfields.ca",
   "productName": "Snowy Fields DLC",
   "main": "main.js",
   "scripts": {
      "start": "electron ."
   },
   "repository": "https://github.com/random-sb/snowyfields-client",
   "keywords": [
      "snowyfields",
      "cpps",
      "club penguin",
      "club penguin private server",
      "anvura"
   ],
   "build": {
      "publish": [
         {
            "provider": "github",
            "owner": "random-sb",
            "repo": "snowyfields-client"
         }
      ],
      "appId": "com.snowyfields.desktop",
      "mac": {
         "asar": false,
         "category": "public.app-category.games"
      },
      "linux": {
         "asar": false,
         "target": [
            {
               "target": "deb",
               "arch": [
                  "x64",
                  "ia32"
               ]
            },
            {
               "target": "AppImage",
               "arch": [
                  "x64",
                  "ia32"
               ]
            }
         ]
      },
      "win": {
         "asar": false,
         "target": [
            {
               "target": "nsis",
               "arch": [
                  "x64",
                  "ia32"
               ]
            }
         ]
      }
   },
   "author": "Snowy Fields (www.snowyfields.ca)",
   "license": "CC0-1.0",
   "devDependencies": {
      "electron": "^8.1.1",
      "electron-builder": "^22.9.1"
   },
   "dependencies": {
      "electron-updater": "^4.3.2",
      "discord-rpc": "^3.1.4"
   }
}
