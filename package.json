{
   "name": "penguinzoneclient",
   "version": "1.0.0",
   "description": "Desktop client for penguinzone.ca",
   "productName": "Penguin Zone",
   "main": "main.js",
   "scripts": {
      "start": "electron ."
   },
   "repository": "https://github.com/Anvura/snowy-fields-client",
   "keywords": [
      "snowy fields",
      "cpps",
      "club penguin",
      "club penguin private server",
      "anvura"
      "penguin zone"
      "pz"
      "sf"
      "mjjrrb905"
      "mj"
   ],
   "build": {
      "publish": [
         {
            "provider": "github",
            "owner": "anvura",
            "repo": "snowy-fields-client"
         }
      ],
      "appId": "com.penguinzone.desktop",
      "mac": {
         "asar": false,
         "category": "public.app-category.games"
      },
      "linux": {
         "asar": false,
         "target": [
            {
               "target": "deb",
               "arch": [
                  "x64",
                  "ia32"
               ]
            },
            {
               "target": "AppImage",
               "arch": [
                  "x64",
                  "ia32"
               ]
            }
         ]
      },
      "win": {
         "asar": false,
         "target": [
            {
               "target": "nsis",
               "arch": [
                  "x64",
                  "ia32"
               ]
            }
         ]
      }
   },
   "author": "Penguin Zone <business.anvurayt@gmail.com> (www.penguinzone.ca)",
   "license": "CC0-1.0",
   "devDependencies": {
      "electron": "^8.1.1",
      "electron-builder": "^22.9.1"
   },
   "dependencies": {
      "electron-updater": "^4.3.2",
      "discord-rpc": "^3.1.4"
   }
}
